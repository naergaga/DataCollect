@model Book

<h5>@Model.Name</h5>
@foreach (var sheet in Model.Sheets)
{
    <h5>@sheet.Name</h5>
    @*<ul class="sheet-item d-flex">
            @foreach (var col in sheet.Columns)
            {
                <li>@col.Name</li>
            }
        </ul>*@
    <table>
        <tr>
            @foreach (var col in sheet.Columns)
            {
                <td>@col.Name</td>
            }
        </tr>
        @if (sheet.Rows != null)
        {
            @foreach (var row in sheet.Rows)
            {
                <tr>
                    @foreach (var data in row.Data)
                    {
                        <td>@data.Value</td>
                    }
                </tr>
            }
        }
    </table>

}
